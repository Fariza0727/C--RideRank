@model RR.Dto.Contest.JoinPrivateContestDto
<div id="response"></div>
<form id="frmJoinPrivateContest" asp-action="GetPrivateContest" asp-controller="Contest" method="POST" data-ajax="true" data-ajax-update="response" data-ajax-begin="onBegin" data-ajax-complete="onComplete"
      data-ajax-failure="onFailed" data-ajax-success="onPrivateContestSuccess">
    <div class="modal-body _body">
        @*<input type="text" name="UniqueCode" required autocomplete="off" class="form-control" />*@
        @Html.TextBoxFor(x => x.Code, new { autocomplete = "off", @class = "form-control" })
        @Html.ValidationMessageFor(x=>x.Code)
    </div>
    <div class="modal-footer">
        <button type="submit" class="btn btn-default">Submit</button>
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
    </div>
</form>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
