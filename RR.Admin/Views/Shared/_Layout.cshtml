<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title - RR.Admin</title>

    @RenderSection("styles", required: false)

    <link href="~/css/sweetalert.css" rel="stylesheet" />
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/css.css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" rel="stylesheet" />
    <link href="~/css/font-awesome.min.css" rel="stylesheet" />    <!-- CSS Files -->
    <link href="~/assets/css/material-dashboard.css?v=2.1.1" rel="stylesheet" />
    <link href="~/assets/css/custom-admin.css" rel="stylesheet" />
    <link href="~/assets/css/material-dashboard.css?v=2.1.1" rel="stylesheet" />
    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link href="~/assets/demo/demo.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="icon" type="image/png" href="~/favicon-32x32.png" sizes="32x32" />
    <link rel="icon" type="image/png" href="~/favicon-16x16.png" sizes="16x16" />
</head>
<body onload="ActiveLink()">
    <div id="preloader">
        <div class="preloader-body">
            <div class="preloader-item"></div>
        </div>
    </div>
    <div class="container">
        <main role="main" class="pb-3">
            <div class="wrapper ">
                <partial name="_SidePanelLayout" />
                <div class="main-panel ps-container ps-theme-default ps-active-y" data-ps-id="f6f110e3-f3ec-98ce-ab02-b01441fffba2">
                    <!-- Navbar -->
                    <partial name="_NavBarLayout" />
                    <!-- End Navbar -->
                    <div class="content">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; @DateTime.Now.Year - RR.Admin - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    @*<script src="~/scripts/jquery.js"></script>*@
    @*<script src="~/scripts/jquery.validate.js"></script>*@
    <script src="~/assets/js/core/jquery.min.js"></script>
    <script src="~/assets/js/plugins/jquery.validate.min.js"></script>
    <script src="~/assets/js/jquery.validate.unobtrusive.min.js"></script>
    
    <script src="~/js/core.min.js" type="text/javascript"></script>
    <script src="~/js/script.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/lib/jquery.unobtrusive-ajax.js"></script>
    <script type="text/javascript" src="~/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/js/dataTables.bootstrap.min.js"></script>
    <script type="text/javascript" src="~/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/js/sweetalert.min.js"></script>
    <script src="~/js/CustomFunction.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
