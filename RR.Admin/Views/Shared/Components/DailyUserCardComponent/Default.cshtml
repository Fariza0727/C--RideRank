@model RR.Dto.UserChartDto

<div class="card card-chart">
    <div class="card-header card-header-success">
        <div class="ct-chart" id="dailyuserChart" 
             data-labels="@string.Join(",", Model.labels)" 
             data-series="@string.Join(",", Model.series)"
             data-max="@Model.high">
        </div>
    </div>
    <div class="card-body">
        <h4 class="card-title">Players Graph</h4>
        <p class="card-category">
            <span class="text-success"><i class="fa fa-long-arrow-up"></i>@Model.parcentage % </span> joint player in week.
        </p>
    </div>
    <div class="card-footer">
        <div class="stats">
            <i class="material-icons">access_time</i> updated 4 minutes ago
        </div>
    </div>
</div>




